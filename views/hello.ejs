<!DOCTYPE html>
<html>
  <head>

    <!-- Latest compiled and minified CSS -->
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">

    <!-- jQuery library -->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.1.1/jquery.min.js"></script>

    <!-- Latest compiled JavaScript -->
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>

    <p>Enter your email</p>
      <form name="custom-contact-form">
        <div class="row col-sm-6">
        <div class="col-sm-10 home-pg-placeholder-div">
        <div class="form-group" style="width: 100%">
                        <input type="text" class="form-control home-pg-placeholder-custom" id="cname" name="name" placeholder="Name">
                    </div>
        </p></div>
        <div class="col-sm-10 home-pg-placeholder-div">
        <div class="form-group" style="width: 100%">
                        <input type="text" class="form-control home-pg-placeholder-custom" id="cmail" name="email" placeholder="Email">
                    </div>
        </p></div>
        <div class="col-sm-10 home-pg-placeholder-div">
        <div class="form-group" style="width: 100%">
                        <input type="text" class="form-control home-pg-placeholder-custom" id="csub" name="subject" placeholder="Subject">
                    </div>
        </p></div>
        <div class="col-sm-10" style="padding: 10px;">
        <div class="form-group">
                        <textarea class="form-control home-pg-textarea-custom" rows="6" id="cmsg" name="message" placeholder="Some Description about your project"></textarea>
        			</div>
        </p></div>
        <div class="col-sm-10" style="padding: 10px;text-align:center">
        		    <button type="submit" class="btn  btn-success home-pg-custom-submit-btn" id="send">Send</button>
        		</div>
        </p></div>
      </form>


    <!-- <input type="emailtext" name="hellotext" id="hellomail" class="Email" placeholder="Type Email">
    <button type="submit" id="send" class="btn btn-success">Send</button> -->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.1.1/jquery.min.js"></script>
    <script type="text/javascript">
    	$('#send').click( function (evt){
    		evt.preventDefault();
        var data = {
    		 name : $('#cname').val(),
    		mail : $('#cmail').val(),
    		subject : $('#csub').val(),
    	  msg : $('#cmsg').val()
        }
    		$.ajax({
    			url : '/a' ,
    			type : 'POST' ,
    			data : data,
    			success : function (result){
    				if(!result.error){
    					alert(result);
    				}
    			}
    		})
    	});
    </script>

  </body>
</html>
